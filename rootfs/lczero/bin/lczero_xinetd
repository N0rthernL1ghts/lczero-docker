#!/usr/bin/env bash
SCRIPT_PATH="$(
    cd "$(dirname "$0")" || exit 1
    pwd -P
)"

cd "${SCRIPT_PATH}" || exit 1

main() {
    echo "> Starting lc0 ${LCZERO_VERSION}"
    echo "  Network: ${LCZERO_NETWORK_SHA}"

    ./lc0 --config=/lczero/resources/lc0.config
    return $?
}

main "${@}"
exit $?
